{
  "name": "toggles-io-client",
  "version": "1.0.0",
  "description": "Single page web application for toggles.io",
  "repository": "https://github.com/JackPu/yarn-react-webpack-seed",
  "author": "Karol Majta <karol@pioneerlabs.io>",
  "license": "GPL",
  "private": true,
  "scripts": {
    "lint": "node_modules/.bin/tslint src/**/*.ts{,x}",
    "test": "node_modules/.bin/jest",
    "build": "node_modules/.bin/webpack",
    "develop": "NODE_ENV=development node_modules/.bin/webpack-dev-server --content-base build/"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@types/radium": "^0.18.23",
    "@types/react": "^16.0.30",
    "@types/react-dom": "^16.0.3",
    "@types/react-router": "^4.0.19",
    "radium": "^0.19.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.5",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^21.1.8",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "html-webpack-plugin": "^2.30.1",
    "interpolate-html-plugin": "^2.0.0",
    "jest": "^21.2.1",
    "raf": "^3.4.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "tslint-react-recommended": "^1.0.15",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
