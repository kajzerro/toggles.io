{
  "name": "toggles-io-client",
  "version": "1.0.0",
  "description": "Single page web application for toggles.io",
  "repository": "https://github.com/pioneerlabs/toggles.io.git",
  "author": "Karol Majta <karol@pioneerlabs.io>",
  "license": "GPL",
  "private": true,
  "scripts": {
    "lint": "tslint src/**/*.ts{,x}",
    "test": "jest",
    "build": "webpack",
    "develop": "NODE_ENV=development webpack-dev-server --content-base build/",
    "deploy": "serverless deploy --verbose && serverless syncToS3 && serverless domainInfo"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@types/es6-promise": "^0.0.33",
    "@types/radium": "^0.18.23",
    "@types/react": "^16.0.30",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^4.0.19",
    "@types/react-router-dom": "^4.2.3",
    "@types/uuid": "^3.4.3",
    "amazon-cognito-identity-js": "^1.29.0",
    "apisauce": "^0.14.3",
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "json-loader": "^0.5.7",
    "radium": "^0.19.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reduxsauce": "^0.7.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.5",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^21.1.8",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "html-webpack-plugin": "^2.30.1",
    "interpolate-html-plugin": "^2.0.0",
    "jest": "^21.2.1",
    "raf": "^3.4.0",
    "serverless": "^1.24.1",
    "serverless-single-page-app-plugin": "file:./../serverless-plugins/serverless-single-page-app-plugin",
    "ts-jest": "^21.2.4",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "tslint-react-recommended": "^1.0.15",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
